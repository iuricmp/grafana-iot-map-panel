{
  "name": "grafana-iot-map-panel",
  "version": "1.0.0",
  "description": "A panel for Grafana that visualizes IOT GPS points on an interactive map.",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "Iuri Pereira",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "7.5.1",
    "@grafana/toolkit": "7.5.1",
    "@grafana/ui": "7.5.1",
    "@types/mapbox-gl": "^2.1.1",
    "emotion": "10.0.27"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "mapbox-gl": "^2.1.1",
    "react-map-gl": "^6.1.10"
  }
}
